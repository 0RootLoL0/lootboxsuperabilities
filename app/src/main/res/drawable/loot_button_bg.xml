<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#707070">
            </stroke>
            <solid
                android:color="#B89200">
            </solid>
            <corners android:radius="90.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#707070">
            </stroke>
            <solid
                android:color="#FFCC00">
            </solid>
            <corners android:radius="90.0dip" />
        </shape>
    </item>
</selector>